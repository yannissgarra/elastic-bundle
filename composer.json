{
    "name": "webmunkeez/elastic-bundle",
    "type": "symfony-bundle",
    "description": "Elasticsearch for Symfony.",
    "keywords": [
        "symfony",
        "autowiring",
        "elasticsearch"
    ],
    "homepage": "https://github.com/yannissgarra/elastic-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Sgarra",
            "email": "hello@yannissgarra.com",
            "homepage": "https://yannissgarra.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "elasticsearch/elasticsearch": "^8.9",
        "symfony/config": "^6.3",
        "symfony/console": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/property-access": "^6.3",
        "symfony/property-info": "^6.3",
        "symfony/runtime": "^6.3",
        "symfony/uid": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/framework-bundle": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Webmunkeez\\ElasticBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmunkeez\\ElasticBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    }
}
